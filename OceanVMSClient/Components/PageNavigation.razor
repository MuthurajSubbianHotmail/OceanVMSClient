<MudBreadcrumbs Items="Items" Class="px-0">
    @foreach (var item in Items)
    {
        <MudBreadcrumbItem Href="@item.Href" Disabled="@item.Disabled" class="px-0 mx-0">@item.Text</MudBreadcrumbItem>
    }
</MudBreadcrumbs>

@code {
    [Parameter]
    public List<BreadcrumbItem> Items { get; set; } = new List<BreadcrumbItem>();
}
